makecint -mk Makefile -dl ../../include/cintsock.dll -h cintsock.h -C cintsock.c -cint -Z0
echo 'mksockh : mksockh.c' >> Makefile
echo '	$(CC) $(SYSMACRO) -o mksockh mksockh.c' >> Makefile

make -f Makefile mksockh
./mksockh
rm -f mksockh
rm -f mksockh.exe

make -f Makefile
rm G__* Makefile
rm *.o


